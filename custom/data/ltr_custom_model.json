{
  "store": "custom_feature",
  "name": "custom_model",
  "class": "org.apache.solr.ltr.model.MultipleAdditiveTreesModel",
  "features": [
    {
      "name": "feature_subject"
    },
    {
      "name": "feature_code"
    },
    {
      "name": "feature_level"
    },
    {
      "name": "table_onehot_id"
    },
    {
      "name": "table_onehot_id_0"
    },
    {
      "name": "table_onehot_id_1"
    },
    {
      "name": "table_multihot_id"
    },
    {
      "name": "table_multihot_id_0"
    },
    {
      "name": "table_multihot_id_1"
    },
    {
      "name": "table_eval_lnglat"
    },
    {
      "name": "table_eval_lnglat_0"
    },
    {
      "name": "table_eval_lnglat_1"
    },
    {
      "name": "table_eval_lnglat_2"
    }
  ],
  "params": {
    "trees": [
      {
        "weight": "1",
        "root": {
          "feature": "feature_level",
          "threshold": "2",
          "left": {
            "feature": "feature_subject",
            "threshold": "0.0",
            "left": {
              "value": "100"
            },
            "right": {
              "value": "200"
            }
          },
          "right": {
            "feature": "table_eval_lnglat_0",
            "threshold": "1.0",
            "left": {
              "value": "300"
            },
            "right": {
              "value": "400"
            }
          }
        }
      },
      {
        "weight": "1",
        "root": {
          "feature": "feature_code",
          "threshold": "1",
          "left": {
            "feature": "table_eval_lnglat_1",
            "threshold": "0.2",
            "left": {
              "value": "1000"
            },
            "right": {
              "value": "2000"
            }
          },
          "right": {
            "feature": "table_eval_lnglat_2",
            "threshold": "0.3",
            "left": {
              "value": "10000"
            },
            "right": {
              "value": "20000"
            }
          }
        }
      }
    ]
  }
}